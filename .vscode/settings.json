{
	"editor.fontFamily": "menlo",
	"editor.fontWeight": "100",
	"editor.fontSize": 13,
	"editor.smoothScrolling": true,
	"jslint.enable": false,
	"files.exclude": {
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.gitkeep": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/*.js.map": true,
		"**/*.min.js": true,
		"**/CVS": true,
		"api/**/*.js": true,
		"**/*.js": {
			"when": "$(basename).ts"
		}
	},
	"cSpell.words": [
		"Ellcrys",
		"passphrase"
	]
}
