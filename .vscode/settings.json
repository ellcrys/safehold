{
	"editor.rulers": [90],
	"editor.fontFamily": "monaco",
	"editor.fontWeight": "100",
	"editor.fontSize": 13,
	"editor.smoothScrolling": true,
	"editor.letterSpacing": 0.7,
	"editor.dragAndDrop": true,
	"editor.minimap.enabled": false,
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 5000,
	"editor.quickSuggestionsDelay": 14,
	"editor.lineHeight": 25,
	"javascript.validate.enable": false,
	"typescript.validate.enable": false,
	"workbench.sideBar.location": "left",
	"editor.trimAutoWhitespace": false,
	"go.formatOnSave": true,
	"workbench.activityBar.visible": true,
	"workbench.statusBar.visible": true,
	"window.zoomLevel": 0,
	"workbench.editor.swipeToNavigate": true,
	"jslint.enable": false,
	"workbench.iconTheme": "ayu",
	"cSpell.language": "en-GB",
	"cSpell.userWords": [],
	"editor.renderIndentGuides": false,
	"git.enableSmartCommit": true,
	"go.editorContextMenuCommands": {
		"toggleTestFile": true,
		"addTags": true,
		"removeTags": false,
		"testAtCursor": true,
		"testFile": true,
		"testPackage": true,
		"generateTestForFunction": true,
		"generateTestForFile": false,
		"generateTestForPackage": false,
		"addImport": true,
		"testCoverage": true
	},
	"go.testTimeout": "50s",
	"go.coverageDecorator": {
		"type": "gutter"
	},
	"files.exclude": {
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.gitkeep": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/*.js.map": true,
		"**/*.min.js": true,
		"**/CVS": true,
		"api/**/*.js": true
	},
	"auto-close-tag.activationOnLanguage": [
		"xml",
		"php",
		"blade",
		"ejs",
		"njk",
		"jinja",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"plaintext",
		"markdown",
		"vue",
		"html",
		"liquid"
	],
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.snippetSuggestions": "top",
	"editor.formatOnPaste": false,
	"editor.formatOnSave": true,
	"editor.tabSize": 4,
	"editor.insertSpaces": true,
	"workbench.colorCustomizations": {
		"editorRuler.foreground": "#313131",
		"statusBar.background": "#000000",
		"editor.background": "#000000",
		"sideBar.background": "#000000",
		"tab.inactiveBackground": "#000000",
		"tab.inactiveForeground": "#313131",
		"tab.activeBackground": "#031c3d",
		"sideBarSectionHeader.background": "#000000",
		"activityBar.background": "#000000",
		"statusBar.noFolderBackground": "#080808",
		"statusBar.debuggingBackground": "#0d1113"
	},
	"typescript.tsdk": "/usr/local/lib/node_modules/typescript/lib/",
	"workbench.colorTheme": "Monokai++",
	"editor.renderLineHighlight": "none",
	"problems.decorations.enabled": false,
	"extensions.ignoreRecommendations": true,
	"emmet.triggerExpansionOnTab": true,
	"workbench.fontAliasing": "default",
	"terminal.integrated.fontWeight": "100",
	"editor.renderWhitespace": "none",
	"breadcrumbs.enabled": true,
	"go.testFlags": [
		"-v"
	],
	"editor.tabCompletion": "on",
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"prettier.arrowParens": "always",
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.printWidth": 90,
	"prettier.eslintIntegration": true,
	"editor.renderControlCharacters": true,
	"terminal.integrated.fontFamily": "Source Code Pro",
	"editor.suggestFontSize": 13,
	"javascript.preferences.quoteStyle": "double",
	"prettier.tslintIntegration": true,
	"go.lintFlags": [
		"-exclude=vendor/...",
		"-config=${workspaceFolder}/revive.toml"
	],
	"cSpell.words": [
		"Ellcrys",
		"passphrase"
	]
}
