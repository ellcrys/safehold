{
	"name": "SafeHold",
	"version": "0.1.4-alpha",
	"repository": {
		"url": "https://github.com/ellcrys/safehold/"
	},
	"author": "Ellcrys <hello@ellcrys.co>",
	"description": "Official desktop wallet and miner for the Ellcrys network",
	"license": "",
	"main": "./dist/electron/main.js",
	"scripts": {
		"build": "npm run copybin:darwin && cross-env NET_ID=0001 NODE_ENV=production node .electron-vue/build.js && electron-builder",
		"build:dir": "npm run copybin:darwin && cross-env NET_ID=0001 NODE_ENV=production node .electron-vue/build.js && electron-builder --dir",
		"build-win": "npm run copybin:windows && cross-env NET_ID=0001 NODE_ENV=production node .electron-vue/build.js && electron-builder -w",
		"build-linux": "npm run copybin:linux && cross-env NET_ID=0001 NODE_ENV=production node .electron-vue/build.js && electron-builder -l",
		"build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
		"build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
		"dev": "npm run copybin:darwin  && node .electron-vue/dev-runner.js",
		"dev-win": "npm run copybin:windows  && node .electron-vue/dev-runner.js",
		"dev-linux": "npm run copybin:linux  && node .electron-vue/dev-runner.js",
		"pack": "npm run pack:main && npm run pack:renderer",
		"pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
		"pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
		"test": "mochapack --webpack-config .electron-vue/webpack.config-test.js './test/**/*.ts'",
		"postinstall2": "npm run lint:fix",
		"postinstall": "electron-builder install-app-deps",
		"copybin:darwin": "cpy binaries/elld-darwin.tar.gz static/bin --rename=elld.tar.gz",
		"copybin:windows": "cpy binaries/elld-windows.tar.gz static/bin --rename=elld.tar.gz",
		"copybin:linux": "cpy binaries/elld-linux.tar.gz static/bin --rename=elld.tar.gz",
		"compile": "gulp compile"
	},
	"build": {
		"productName": "SafeHold",
		"appId": "org.ellcrys.safehold",
		"directories": {
			"output": "build"
		},
		"files": [
			"dist/electron/**/*"
		],
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"mac": {
			"icon": "build/icons/icon.icns"
		},
		"win": {
			"icon": "build/icons/icon.ico",
			"target": [
				"nsis",
				"msi"
			]
		},
		"linux": {
			"icon": "build/icons",
			"target": [
				"AppImage",
				"deb"
			]
		}
	},
	"dependencies": {
		"@chenfengyuan/vue-carousel": "^1.0.1",
		"@dicebear/avatars": "^3.0.6",
		"@dicebear/avatars-identicon-sprites": "^2.0.6",
